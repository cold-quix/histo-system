#
# =======================================================
#                     Data Consumer
# =======================================================
# FINAL BINARY DC
./bin/DC : ./obj/DC.o
	cc ./obj/DC.o -o ./bin/DC
#
# =======================================================
#                     Dependencies
# =======================================================
./obj/DC.o : ./src/consumer.c ./inc/prototypes.h ../inc/constants.h
	cc -c ./src/consumer.c -o ./obj/DC.o

#
# =======================================================
# Other targets
# =======================================================
clean:
	rm -f ./bin/*
	rm -f ./obj/*
	rm -f ./src/consumer.c~
